#!/bin/bash

#shell
alias ll='ls -l'
alias cl='clear'
alias md5sum='md5 -r'
export GREP_OPTIONS="--exclude-dir=\.svn --exclude-dir=\.git --exclude-dir=dist --exclude-dir=node_modules --color"

#git
alias gpu='sh ~/git.sh push'
alias gpl='sh ~/git.sh pull'
alias gs='git status'
alias gd='git diff'
alias gb='git branch'

#node
alias ni='npm i'
alias nci='npm ci'
export NODE_ENV='development'
